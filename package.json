{
  "name": "serverless-next",
  "version": "1.0.0",
  "scripts": {
    "fmt": "tslint -c tslint.json --project tsconfig.json --fix 'src/**/*.{ts,tsx}'",
    "dev": "nodemon",
    "offline": "yarn build && serverless offline",
    "build": "next build ./src && tsc -p tsconfig.server.json",
    "start": "next start ./src",
    "deploy": "yarn build && serverless deploy",
    "type-check": "tsc ./src"
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "express": "^4.16.4",
    "next": "^8.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.0",
    "@types/express": "^4.16.1",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.10",
    "@types/react-dom": "16.8.3",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "serverless": "^1.40.0",
    "serverless-offline": "^4.9.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "3.4.1"
  },
  "license": "ISC"
}
